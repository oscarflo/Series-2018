asalariado315 <- subset(sdemt315,ENT==25 & REMUNE2C==1) #REMUNE2C==1- subordinados y remunerados-asalariados
informal315 <- subset(sdemt315,ENT==25 & TUE2==5)# TUE2==5 - sector informal
informals315<-nrow(informal315)
pobocups315<- nrow(pobocup315)
til1315<-(informals315/pobocups315)*100
til1315 ## TASA DE INFORMALIDAD LABORAL
asalariados315<-nrow(asalariado315)
tasaasariados315<-(asalariados315/pobocups315)*100
tasaasariados315 ### TASA DE ASALARIADOS
ocupados315 <-nrow(ocupado315)
desocupados315 <-nrow(desocupado315)                  
tasadesocupacion315 <- (desocupados315/ocupados315)*100
tasadesocupacion315

